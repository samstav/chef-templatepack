    "rails": {
      "help": "Creates a rails application recipe based on the type, defaults to Apache"
    },
    "rabbitmq": {
      "help": "Creates a rabbitmq server"
    },
    "redis": {
      "help": "Creates a redis recipe"
    },
    "memcached": {
      "help": "Creates a memcached recipe"
    },
    "django": {
      "help": "Creates a django application"
